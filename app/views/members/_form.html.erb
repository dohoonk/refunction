<%= form_for(@member) do |f|%>
  <%= f.file_field :image %>

  <%= f.label "First Name" %>
  <%= f.text_field :first_name %>

  <%= f.label "Middle Name" %>
  <%= f.text_field :middle_name %>

  <%= f.label "Last Name" %>
  <%= f.text_field :last_name %>

  <%= f.label "Description" %>
  <%= f.text_area :description %>

<% end %>

<div class="col-sm-3">
  <div class="row justify-content-between">
    <div class="list_of_services_box">
      <h5>Learn about our services</h5>
      <div class="highlight"></div>
      <ul>
        <% @services.each do |f| %>
          <li><%= link_to "#{f.title}", service_path(f) %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
